{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-useless-catch": 0,
        "indent": ["error", 4]
    }
}
